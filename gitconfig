[push]
	default = simple
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%C(bold blue)<%an>%Creset' --abbrev-commit
	type = cat-file -t
	dump = cat-file -p
[credential]
	helper = cache --timeout=3600
[pull]
	rebase = preserve
